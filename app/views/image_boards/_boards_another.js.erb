//= require 'modal_boards'

console.log('AJAXing: '+"<%= @id %>");
selector = "#" + "<%= @id.to_s %>";
console.log(selector);

$(selector).popover('destroy');
$(selector).popover({
  html: true,
  content: "<%= escape_javascript(render partial: 'shared/popover_board', locals: { image: @image, image_board: @board, html: @id }) %>",
  container: 'body',
  trigger: 'manual'
}).popover('show');