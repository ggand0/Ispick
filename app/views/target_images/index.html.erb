<h1>Listing target_images</h1>
<%= link_to 'New Target image', new_target_image_path %>
<br>

<% @target_images.each do |target_image| %>
  <div class="box">
    <div class="boxInner">
      <%= link_to 'Show', target_image %>
      <%= link_to 'Similar', prefer_target_image_path(target_image) %>
      <%= link_to 'Similar ConvNet', similar_convnet_target_image_path(target_image) %>
      <%= link_to 'Edit', edit_target_image_path(target_image) %>
      <%= link_to 'Destroy', target_image, method: :delete, data: { confirm: 'Are you sure?' } %>
      <%= link_to(image_tag(target_image.data.url(:thumb)), { controller: 'target_images', action: 'show', id: target_image.id.to_s }) %>
      <div class="titleBox"><%= target_image.data.content_type %></div>
    </div>
  </div>
<% end %>

<%= paginate @target_images %>
