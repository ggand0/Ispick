<h1>Listing images</h1>

<div class="wrap">

  <!-- Define all of the tiles: -->
  <div class="box">
    <div class="boxInner">
      <img src="http://www.dwuser.com/education/content/creating-responsive-tiled-layout-with-pure-css/images/demo/7.jpg" />
      <div class="titleBox">Butterfly</div>
    </div>
  </div>
  <% @images.each do |image| %>
    <div class="box">
      <div class="boxInner">
        <%= image_tag image.data.url %>
        <div class="titleBox"><%= image.title.to_s %></div>
      </div>
    </div>
  <% end %>
</div>